{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CueNoteContracts",
  "type": "object",
  "properties": {
    "job": {
      "type": "object",
      "properties": {
        "id": { "type": "integer" },
        "name": { "type": "string" },
        "status": { "type": "string", "enum": ["queued", "running", "completed", "failed"] }
      },
      "required": ["id", "name", "status"],
      "additionalProperties": false
    }
  }
}
